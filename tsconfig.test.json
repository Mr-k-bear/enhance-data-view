{
    "extends": "@tsconfig/node-lts/tsconfig.json",
    "include": [
        "test/**/*"
    ],
    "compilerOptions": {
        "noEmit": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.test.tsbuildinfo",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "paths": {
            "enhance-data-view": [
                "./src/index.ts"
            ]
        },
        "types": [
            "node",
            "@types/jest"
        ]
    }
}